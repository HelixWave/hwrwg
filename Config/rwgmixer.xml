		<prefab name="cabin_05" />
		<prefab name="cabin_06" />
		<prefab name="cabin_07"/>
		<prefab name="cabin_08"/>
		<prefab name="cabin_09"/>
		<prefab name="cabin_10"/>
		<prefab name="cabin_08"/>
		<prefab name="cabin_12"/>
